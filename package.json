        "karma": "^6.3.17",
        "karma-coverage": "^2.2.0",
        "karma-firefox-launcher": "^2.1.2",
        "karma-jasmine": "^4.0.1",
        "sinon": "^13.0.1",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2",
        "clean-webpack-plugin": "^4.0.0",
